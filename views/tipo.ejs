<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <title>Tabla de Tipo</title>
    <link href="/stylesheets/style.css" rel="stylesheet" type="text/css">
</head>
<body>
    <h1>Tabla de Tipo</h1>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Tipo</th>
            </tr>
        </thead>
        <tbody>
            <% tipoData.forEach(tipo => { %>
                <tr>
                    <td><%= tipo.tipoId %></td>
                    <td><%= tipo.tipoCol %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
